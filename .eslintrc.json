{
    "env": {
        "es6": true,
        "node": true,
        "jest/globals": true
    },
    "extends": [
      "eslint:recommended", 
      "prettier", 
      "sketch"],
    "plugins": ["prettier", "jest"],
    "globals": {
        "BCSketchInfo": "readonly",
        "context": "readonly",
        "nil": "readonly",
        "MOPointer": "readonly",
        "NSURLConnection": "readonly",
        "NSURLRequest": "readonly",
        "NSURLSession": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "no-dupe-else-if": "off",
      "no-import-assign": "off",
      "no-setter-return": "off"
    }
}